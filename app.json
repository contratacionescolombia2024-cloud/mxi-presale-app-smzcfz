
{
  "expo": {
    "name": "MXI Presale",
    "slug": "mxi-presale-app-smzcfz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/effa8fd0-8607-4a97-b8f0-88a8462f2535.png",
    "scheme": "mxipresale",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/effa8fd0-8607-4a97-b8f0-88a8462f2535.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mxipresale.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/effa8fd0-8607-4a97-b8f0-88a8462f2535.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mxipresale.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/effa8fd0-8607-4a97-b8f0-88a8462f2535.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
