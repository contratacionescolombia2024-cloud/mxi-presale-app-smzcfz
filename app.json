
{
  "expo": {
    "name": "Mxi Presale",
    "slug": "Mxi Presale",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/439b868e-5ae8-4439-8324-ce6876084e5f.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/439b868e-5ae8-4439-8324-ce6876084e5f.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "scheme": "mxipresale",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/439b868e-5ae8-4439-8324-ce6876084e5f.png",
        "backgroundColor": "#1a1a1a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mxistrategicholdngs.mxistrategic"
    },
    "web": {
      "favicon": "./assets/images/439b868e-5ae8-4439-8324-ce6876084e5f.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
